{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ReverseProxy": {
    "Routes": {
      "all-to-order": {
        "ClusterId": "order",
        "Match": { "Path": "/{**catch-all}" }
      }
    },
    "Clusters": {
      "order": {
        "Destinations": {
          "d1": { "Address": "https+http://orderservice/" }
        }
      }
    }
  }
}
